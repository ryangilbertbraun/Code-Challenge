{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"],
  "exclude": [
    "node_modules",
    "supabase/functions",
    "**/*.stories.tsx",
    "**/__tests__",
    "storybook-app.tsx",
    "components/ui copy"
  ]
}
